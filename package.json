      {
        "command": "abracadabra.splitIfStatement",
        "title": "Split If Statement",
        "category": "Abracadabra"
      }
    ],
    "keybindings": [
      {
        "command": "abracadabra.extractVariable",
        "key": "ctrl+alt+v",
        "mac": "cmd+alt+v",
        "when": "editorTextFocus"
      },
      {
        "command": "abracadabra.inlineVariableOrFunction",
        "key": "ctrl+alt+n",
        "mac": "cmd+alt+n",
        "when": "editorTextFocus"
      },
      {
        "command": "abracadabra.moveStatementDown",
        "key": "ctrl+shift+down",
        "mac": "cmd+shift+down",
        "when": "editorTextFocus"
      },
      {
        "command": "abracadabra.moveStatementUp",
        "key": "ctrl+shift+up",
        "mac": "cmd+shift+up",
        "when": "editorTextFocus"
      }
    ],
